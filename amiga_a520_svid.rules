
(rules PCB amiga_a520_svid
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 492)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.8)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.6)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (padstack "Via[0-1]_1200:800_um"
    (shape
      (circle F.Cu 1200.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 1200.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_1200:800_um" "Via[0-1]_1200:800_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via 
    "Via[0-1]_1200:800_um-kicad_default" "Via[0-1]_1200:800_um" "kicad_default"
  )
  (via_rule
    default "Via[0-1]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    GND +5V "-5V" "Net-(C11-Pad1)" "Net-(C12-Pad2)" BURST VSYNC HSYNC
    CSYNC /B /G "Net-(J1-Pad1)" "Net-(Q1-Pad3)" "Net-(Q1-Pad2)" Rb "Net-(R1-Pad1)"
    "Net-(R2-Pad2)" Cr "Net-(R5-Pad2)" Gb "Net-(R7-Pad1)" Bb "Net-(R10-Pad1)" "Net-(R12-Pad2)"
    Y "Net-(R15-Pad2)" "Net-(R17-Pad1)" "Net-(R19-Pad1)" "Net-(R20-Pad2)" Cb "Net-(R23-Pad2)" "Net-(R25-Pad1)"
    "Net-(R37-Pad2)" COMP "Net-(R38-Pad2)" CRMA "Net-(R39-Pad2)" LUMA "Net-(Co1-Pad1)" "Net-(Co2-Pad1)"
    "Net-(Co3-Pad1)" "Net-(Co1-Pad2)" "Net-(Co2-Pad2)" "Net-(Co3-Pad2)" "Net-(R4-Pad1)" "Net-(R14-Pad1)" "Net-(R22-Pad1)" "Net-(J8-Pad1)"
    "Net-(J10-Pad1)" "Net-(J12-Pad1)" "Net-(J16-Pad1)" "Net-(J18-Pad1)" "Net-(J24-Pad1)" "Net-(J26-Pad1)"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)